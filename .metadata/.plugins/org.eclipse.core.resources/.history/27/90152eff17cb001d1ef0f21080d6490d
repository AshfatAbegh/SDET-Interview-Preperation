package JavaProgrames;

public class Print_1_to_100_Without_Loop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int num = 1;
        
        printnumbers(num);
	}
	
	public static void printnumbers(int num)
	{
		if(num<=100)
		{
			 System.out.println(num + " ");
			 printnumbers(num+1);
		}
	}

}
